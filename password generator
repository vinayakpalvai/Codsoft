import random
print("no of elements required:")
n=int(input())
all_characters="a b c d e f g h i j k l m n o p q r s t u v w x y z 1 2 3 4 5 6 7 8 9 0 # @ ! ~ ? | \ "
all_characters=all_characters.split(" ")
generated_password=""
for i in range(n):
    generated_password=generated_password+all_characters[random.randint(0,43)]
print(generated_password)
